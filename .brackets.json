{
    "brackets-file-tree-exclude.excludeList": [
        "/.git/",
        "/dist/",
        "/public",
        "/bower_components/",
        "/node_modules/",
        ".sass-cache/"
    ],
    "hirse.beautify.onSave": false,
    "path": {
        "thirdparty/**.js": {
            "hirse.beautify.onSave": false
        },
        "**min**.js": {
            "hirse.beautify.onSave": false
        }
    },
    "language": {
        "php": {
            "hirse.beautify.onSave": false
        }
    }
}